FROM deepseek-r1:32b

# No SYSTEM prompt — DeepSeek R1 performs best without one.
# All context (identity, capabilities, rules) is injected into the user message
# by identity/self_model.py get_system_prompt_context().

PARAMETER num_ctx 8192
PARAMETER temperature 0.6
PARAMETER stop <｜begin▁of▁sentence｜>
PARAMETER stop <｜end▁of▁sentence｜>
PARAMETER stop <｜User｜>
PARAMETER stop <｜Assistant｜>
